{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://server.withfestival.site/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://server.withfestival.site/:path*/"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://server.withfestival.site/api/$1"
    }
  ]
}
